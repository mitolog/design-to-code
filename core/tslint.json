{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "linterOptions": {
    "exclude": ["node_modules/**"]
  },
  "rules": {
    "quotemark": [true, "single"],
    "trailing-comma": false,
    "typedef": [
      true,
      "parameter",
      "arrow-parameter",
      "property-declaration",
      "member-variable-declaration"
    ]
  },
  "rulesDirectory": []
}
