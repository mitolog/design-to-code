{
  "name": "sketchlinter",
  "version": "0.1.0",
  "description": "Linter for sketch.",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "sketchLinter",
    "manifest": "src/manifest.json",
    "main": "sketchlinter.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "resources": [
      "resources/**/*.js"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "dependencies": {
    "file-system": "^2.2.2",
    "sketch-module-web-view": "^1.2.3"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2",
    "@skpm/extract-loader": "^2.0.2",
    "child_process": "^1.0.2",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "gulp": "^4.0.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.0",
    "html-loader": "^0.5.5",
    "typescript": "^3.1.4",
    "url-loader": "^1.1.1",
    "webpack": "^4.23.1"
  },
  "author": "mito_log <hkyhm5l@gmail.com>"
}
